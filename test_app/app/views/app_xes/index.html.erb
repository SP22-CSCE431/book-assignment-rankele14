<p id="notice"><%= notice %></p>

<h1>App Xes</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Published</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_xes.each do |app_x| %>
      <tr>
        <td><%= app_x.title %></td>
        <td><%= app_x.published %></td>
        <td><%= link_to 'Show', app_x %></td>
        <td><%= link_to 'Edit', edit_app_x_path(app_x) %></td>
        <td><%= link_to 'Destroy', app_x, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App X', new_app_x_path %>
